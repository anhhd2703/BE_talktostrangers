    "version": "0.2.0",
    "configurations": [
        {
            "name": "T2S",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/biz",
            "args": [
                "-c",
                "${workspaceFolder}/configs/biz.toml"
            ]
        },
        {
            "name": "SFU",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/sfu/signal/allrpc",
            "args": [
                "-jaddr",
                ":7000",
                "-gaddr",
                ":50051",
                "-c",
                "${workspaceFolder}/configs/sfu.toml"
            ]
        }
    ]
}